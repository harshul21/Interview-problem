/*
for n=5
1
01
101
0101
10101
*/
#include<iostream>
using namespace std;
int main()
{
    int n;
    cin>>n;
    for(int i=1;i<=n;i++)
    {
        for(int j=1;j<=i;j++)
        {
            if((abs(i-j))%2!=0)
            {
                cout<<"0";
            }
            if((abs(i-j))%2==0)
            {
                cout<<"1";
            }
        }
        cout<<endl;
    }
    return 0;
}
